<div class="col-md-8">
    <div class="contact-form">
        {{ form_start(form) }}
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <fieldset>
                    {{ form_widget(form.nomGagnant) }}
                </fieldset>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12">
                <fieldset>
                    {{ form_widget(form.prenomGagnant) }}
                </fieldset>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12">
                <fieldset>
                    {{ form_widget(form.adresseGagnant) }}
                </fieldset>
            <fieldset>
                <button class="filled-button">{{ button_label|default('Save') }}</button>
            </fieldset>
        </div>
    </div>
</div>
{{ form_end(form) }}
</div>
</div>
